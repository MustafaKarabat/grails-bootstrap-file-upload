<plugin name='bootstrap-file-upload' version='2.1.1' grailsVersion='2.2.2 &gt; *'>
  <author>Sebastien Arbogast</author>
  <authorEmail>sebastien.arbogast@epseelon.com</authorEmail>
  <title>Bootstrap File Upload Plugin</title>
  <description>Grails Plugin to integrate Sebastian Tschan's Jquery File Upload (https://github.com/blueimp/jQuery-File-Upload).
</description>
  <documentation>https://github.com/sarbogast/grails-bootstrap-file-upload/blob/master/README.md</documentation>
  <type>BootstrapFileUploadGrailsPlugin</type>
  <resources>
    <resource>BootstrapFileUploadResources</resource>
    <resource>org.grails.plugins.bootstrap.file.upload.ImageController</resource>
    <resource>org.grails.plugins.bootstrap.file.upload.BootstrapFileUploadTagLib</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='http://download.java.net/maven/2/' url='http://download.java.net/maven/2/' />
    <repository name='http://grails.org/plugins' url='http://grails.org/plugins' />
    <repository name='http://repo.grails.org/grails/libs-releases' url='http://repo.grails.org/grails/libs-releases/' />
    <repository name='http://m2repo.spockframework.org/ext/' url='http://m2repo.spockframework.org/ext/' />
  </repositories>
  <dependencies>
    <test>
      <dependency group='org.seleniumhq.selenium' name='selenium-chrome-driver' version='2.27.0' />
      <dependency group='org.seleniumhq.selenium' name='selenium-support' version='2.27.0' />
      <dependency group='org.seleniumhq.selenium' name='selenium-htmlunit-driver' version='2.27.0' />
      <dependency group='org.gebish' name='geb-spock' version='0.9.0-RC-1' />
      <dependency group='org.seleniumhq.selenium' name='selenium-firefox-driver' version='2.27.0' />
    </test>
    <build>
      <dependency group='org.apache.httpcomponents' name='httpcore' version='4.1.2' />
      <dependency group='org.apache.httpcomponents' name='httpclient' version='4.1.2' />
    </build>
  </dependencies>
  <plugins>
    <test>
      <plugin group='org.grails.plugins' name='functional-test-development' version='0.9.3' />
      <plugin group='org.grails.plugins' name='cucumber' version='0.8.0' />
      <plugin group='org.grails.plugins' name='geb' version='0.9.0-RC-1' />
    </test>
    <compile>
      <plugin group='org.grails.plugins' name='remote-control' version='1.4' />
    </compile>
    <runtime>
      <plugin group='org.grails.plugins' name='jquery-ui' version='1.8.24' />
      <plugin group='org.grails.plugins' name='fields' version='1.3' />
      <plugin group='org.grails.plugins' name='twitter-bootstrap' version='2.3.2' />
      <plugin group='org.grails.plugins' name='resources' version='1.2' />
      <plugin group='org.grails.plugins' name='jquery' version='1.8.3' />
      <plugin group='org.grails.plugins' name='hibernate' version='2.2.2' />
    </runtime>
    <build>
      <plugin group='org.grails.plugins' name='tomcat' version='2.2.2' />
    </build>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>